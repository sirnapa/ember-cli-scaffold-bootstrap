{{#em-form action="save" on="submit" submitButton=false}}
  <% attrs.forEach(function(attr) { %>
    {{em-input label="<%= attr.label %>" property="<%= attr.name %>" model=model}}
  <% }); %>
  <button type="submit" class="btn btn-success">Submit</button>
  {{link-to "Bank list" "banks.index" tagName="button" class="btn btn-primary"}}
{{/em-form}}
